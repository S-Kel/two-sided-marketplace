<!DOCTYPE html>
<html>
  <head>
    <title>SportsTime</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

      <%= javascript_include_tag "https://js.stripe.com/v2/", "https://js.stripe.com/v3/", 'application', 'data-turbolinks-track': 'reload' %>

    <%# <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload'  %>
    

    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet">

    <%= tag :meta, name: "stripe-public-key", content: Rails.configuration.stripe[:publishable_key] %>
    
  </head>

  <body>
  
  <%= render 'shared/navigation' %>
  <%= render 'shared/message' %>

  <div class="container">

    <%= yield %>
    </div>
      <%= render 'shared/footer' %>

  </body>
</html>
