<div class="panel panel-default">
  <% @coach.each do |k,n| %>
    <ul class="media-list md-6">
        <li class="media">
          <div class="media-left">
            <%= image_tag(n[:image], class: "media-object") %>
          </div>
          <div class="media-body md-6">
            <h4 class="card-title"><%= n[:name] %></h4>
            <h5 class="card-title"><%= n[:city] %></h5>
            <p class="card-text"><%= n[:description] %></p>
            <%= link_to('Buy',class: "btn btn-warning" ) %>
          </div>
        </li>
      </ul>
  <% end %>
</div>